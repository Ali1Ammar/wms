{
  "cSpell.words": [
    "appbundle",
    "Cupertino",
    "fluttergen",
    "fpdart",
    "wms",
    "mocktail",
    "riverpod"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "pubspec.lock",
    "script_runner.yaml"
  ],
  "explorer.fileNesting.enabled": true,
  "editor.stickyScroll.enabled": true,
  "explorer.fileNesting.expand": false,
  "editor.codeActionsOnSave": { "source.fixAll": true },

  "explorer.fileNesting.patterns": {
    "pubspec.yaml": ".flutter-plugins, .packages, .dart_tool, .flutter-plugins-dependencies, .metadata, .packages, pubspec.lock,all_lint_rules.yaml,custom_lint.log,l10n.yaml,wms.iml,analysis_options.yaml",
    ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
    "readme.*": "authors, backers.md, changelog*, citation*, code_of_conduct.md, codeowners, contributing.md, contributors, copying, credits, governance.md, history.md, license*, maintainers, readme*, security.md, sponsors.md",
    "*.dart": "$(capture).g.dart, $(capture).freezed.dart, $(capture).gr.dart",
    "build.yaml": "build.*.yaml"
  },
  "conventionalCommits.scopes": ["structure", "auth", "ui"]
}
